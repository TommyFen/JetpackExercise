<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="cn.tommyfen.mvvmsample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.LayoutBindExpActivity"
            android:label="LayoutBindExp"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".view.ObservableDataActivity"
            android:label="ObservableData"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".view.BindingAdapterActivity"
            android:label="BindingAdapter"
            android:parentActivityName=".MainActivity" />

        <activity
            android:name=".view.RecyclerViewActivity"
            android:label="RecyclerView"
            android:parentActivityName=".MainActivity" />

    </application>

</manifest>